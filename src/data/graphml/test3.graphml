<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
                             http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">

  <!-- 定义顶层图 -->
  <graph id="G" edgedefault="undirected">
    
    <!-- 单个节点 -->
    <node id="n0"/>
    <node id="n1"/>
    
    <!-- 带属性的节点 -->
    <node id="n2">
      <data key="label">Node 2</data>  <!-- 可以存储节点标签 -->
    </node>

    <!-- 复合节点 / 嵌套图 (cluster) -->
    <node id="n3">
      <graph id="n3_subgraph" edgedefault="directed"> <!-- 嵌套图可以是有向或无向 -->
        <node id="n3::n0"/>
        <node id="n3::n1"/>
        <edge id="e_sub1" source="n3::n0" target="n3::n1"/>
      </graph>
    </node>

    <!-- 带属性的边 -->
    <edge id="e0" source="n0" target="n1">
      <data key="weight">1.5</data>  <!-- 边的权重 -->
    </edge>

    <!-- 顶层边连接复合节点内部节点 -->
    <edge id="e1" source="n1" target="n3::n0"/>

    <!-- 有向边 -->
    <edge id="e2" source="n2" target="n0"/>

    <!-- 自环边 -->
    <edge id="e3" source="n2" target="n2"/>

  </graph>

  <!-- 定义节点属性 key -->
  <key id="label" for="node" attr.name="label" attr.type="string"/>
  <!-- 定义边属性 key -->
  <key id="weight" for="edge" attr.name="weight" attr.type="double"/>

</graphml>
